  target 'MovieDBAppTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire'
    pod 'SDWebImage'
    pod 'MRProgress'
    pod 'ReachabilitySwift'
    pod 'Cosmos'
    pod 'XCDYouTubeKit'
  end

  target 'MovieDBAppUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire'
    pod 'SDWebImage'
    pod 'MRProgress'
    pod 'ReachabilitySwift'
    pod 'Cosmos'
    pod 'XCDYouTubeKit'
  end

end
